<h1>Registrare</h1>
<% include ./partials/messages %>
<form action="/users/register" method="POST">
   <p><label for="name">Nume:</label>
   <input type="name" id="name" name="name" placeholder="Numele" value="<%typeof name !='undefined' ? name : ' '%>"></div></p>
   <p><label for="email">Email:</label>
   <input type="email" id="email" name="email" placeholder="Email:"value="<%typeof email !='undefined' ? email : ' '%>"></div></p>
   <p><label for="password">Introduceti parola:</label>
   <input type="password" id="password" name="password" placeholder="Introduceti parola:" value="<%typeof password !='undefined' ? password : ' '%>"></div></p>
   <p><label for="password2">Confirmati parola:</label>
   <input type="password" id="password2" name="password2" placeholder="Confirmati parola:" value="<%typeof password2 !='undefined' ? password2 : ' '%>"></div></p>
   <button  type="submit">Registrarea</button>
</form>
<p><a href="/users/login">Logare</a></p>