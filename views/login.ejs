<h1>Logare</h1>
<% include ./partials/messageses %>
<form action="/users/login" method="POST">
   <p><label for="email">Email:</label>
      <input type="email" id="email" name="email" placeholder="Email:"></div>
   </p>
   <p><label for="password">Introduceti parola:</label>
